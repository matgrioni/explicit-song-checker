{
    "manifest_version": 2,

    "name": "Explicit Song Checker",
    "description": "Checks a songs lyrics against explicit words",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Check song lyrics"
    },

    "permissions": [
        "activeTab",
        "http://*/"
    ]
}
